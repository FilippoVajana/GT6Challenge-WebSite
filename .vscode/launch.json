{
    "version": "0.2.0",
    "configurations": [          
        {
            "type": "chrome",
            "request": "launch",
            "name": "Nginx Docker",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceRoot}",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ]
        },
        {            
            "name": "Launch index.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/index.html",
            "port": 9222,
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ]
        }      

    ]
}